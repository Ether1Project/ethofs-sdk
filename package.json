{
  "name": "@ethofs/sdk",
  "version": "1.0.1",
  "description": "Official SDK for the ethoFS Platform",
  "main": "lib/ethofs-sdk.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "coverage": "jest --coverage",
    "dev": "webpack --progress --colors --watch --env dev",
    "html": "open coverage/index.html",
    "lint": "eslint src --quiet ",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "repl": "node -i -e \"$(< ./lib/ethofs-sdk.js)\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ether1Project/ethofs-sdk.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Matt Ober",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ether1Project/ethofs-sdk/issues"
  },
  "homepage": "https://github.com/Ether1Project/ethofs-sdk",
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/register": "^7.12.1",
    "acorn": "^7.4.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-polyfill": "^6.26.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.4",
    "jest": "^25.5.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "base-path-converter": "^1.0.2",
    "bl": "^4.0.3",
    "bs58": "^4.0.1",
    "buffer": "^6.0.3",
    "electron": "^11.0.3",
    "form-data": "^2.3.3",
    "fs": "0.0.1-security",
    "ipfs-http-client": "^48.1.2",
    "is-ipfs": "^0.6.0",
    "recursive-fs": "^1.1.2",
    "swarm-js": "^0.1.40",
    "web3": "^1.3.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "html"
    ],
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "web.js",
      "json",
      "mjs",
      "js",
      "node"
    ]
  }
}
