{
  "name": "@ethofs/sdk",
  "version": "1.0.10",
  "description": "Official SDK for the ethoFS Platform and Etho Protocol Network",
  "main": "src/index.js",
  "scripts": {
    "build-web": "webpack --env mode=production",
    "dev-web": "webpack serve --env mode=development",
    "dev-node": "node ./src-dev/node/index.js",
    "dev-examples": "node ./src-dev/node/examples.js",
    "html": "open coverage/index.html",
    "lint": "eslint src --quiet ",
    "lint:fix": "eslint src --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ether1Project/ethofs-sdk.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Matt Ober",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ether1Project/ethofs-sdk/issues"
  },
  "homepage": "https://github.com/Ether1Project/ethofs-sdk",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "electron": "^13.0.1",
    "eslint": "^7.27.0",
    "eslint-loader": "^4.0.2",
    "html-webpack-plugin": "^4.5.2",
    "inquirer": "^8.1.0",
    "jest": "^25.5.4",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@metamask/detect-provider": "^1.2.0",
    "axios": "^0.21.1",
    "bs58": "^4.0.1",
    "ipfs-http-client": "^48.1.2",
    "is-ipfs": "^0.6.0",
    "web3": "^1.3.6"
  },
  "browserslist": [
    "last 5 version, not dead"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "html"
    ],
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "web.js",
      "json",
      "mjs",
      "js",
      "node"
    ]
  }
}
